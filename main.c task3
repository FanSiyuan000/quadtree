#include "stdio.h"
#include "stdlib.h"
#include "math.h"
#include "treeStructure.h"
#include "buildTree.h"
#include "writeTree.h"
int main(int argc, char **argv) {

	Node *head;

	// make the head node
	head = makeNode(0.0, 0.0, 0);

	// make a tree
	growTree(head);
	growTree(head);
	growTree(head);
	make_leaf1(head);
	make_leaf2(head);
	//makeChildren(head);
	//makeChildren(head->child[1]);
	//removeChildren(head);
	//removeChildren(head->child[1]->child[0]);
	// print the tree for Gnuplot
	//destroyTree(head);
	writeTree(head);
	return 0;
}
